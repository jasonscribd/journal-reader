{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-message",
    "sql:default",
    "sql:allow-load",
    "sql:allow-execute",
    "fs:default",
    "fs:allow-read-file",
    "fs:allow-read-dir",
    "shell:default",
    "shell:allow-open"
  ]
}
